{
  "name": "@pretty-calendar/js",
  "version": "0.1.1",
  "description": "Pretty Calendar JS Library",
  "main": "lib/index.js",
  "scripts": {
    "build:css": "node-sass src/scss/main.scss lib/index.css --output-style=compressed",
    "build": "npm run build:css && tsc",
    "test": "jest --config jestconfig.json",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evgenytk/pretty-calendar.git"
  },
  "keywords": [
    "calendar",
    "pretty",
    "datepicker",
    "js"
  ],
  "author": "evgenytk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/evgenytk/pretty-calendar/issues"
  },
  "homepage": "https://github.com/evgenytk/pretty-calendar#readme",
  "devDependencies": {
    "@pretty-calendar/core": "file:../pretty-calendar-core",
    "node-sass": "^4.11.0"
  }
}
