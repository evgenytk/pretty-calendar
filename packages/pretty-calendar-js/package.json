{
  "name": "@pretty-calendar/js",
  "version": "0.3.0",
  "description": "Pretty Calendar JS Library",
  "main": "lib/index.js",
  "module": "lib/index.es.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build:css": "node-sass src/scss/main.scss lib/index.css --output-style=compressed",
    "dev": "rollup -cw",
    "build": "npm run build:css && rollup -c",
    "test": "jest --config jestconfig.json",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evgenytk/pretty-calendar.git"
  },
  "keywords": [
    "calendar",
    "pretty",
    "datepicker",
    "js"
  ],
  "author": "evgenytk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/evgenytk/pretty-calendar/issues"
  },
  "homepage": "https://github.com/evgenytk/pretty-calendar#readme",
  "dependencies": {
    "@pretty-calendar/core": "^0.3.0"
  },
  "devDependencies": {
    "node-sass": "^4.12.0"
  },
  "gitHead": "ece0f4a868101ca32038ccedcd8217c6b77d6ecb",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib"
  ]
}
